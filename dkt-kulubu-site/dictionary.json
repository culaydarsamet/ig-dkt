[
  {"term":"Afazi","definition":"Dil edinimi veya konuşma yeteneğinde, sıklıkla beyin hasarına bağlı olarak ortaya çıkan bozulma."},
  {"term":"Artikülasyon","definition":"Konuşma seslerini doğru üretme yeteneği; harf ve seslerin ağız yapısıyla oluşturulması."},
  {"term":"Apraksi (Konuşma Apraxisi)","definition":"Konuşma motor planlamasında bozukluk; istemli konuşma hareketlerinin yapılamaması."},
  {"term":"Dizartri","definition":"Sinir sistemi veya kas kontrolündeki bozukluk nedeniyle ortaya çıkan konuşma güçlüğü."},
  {"term":"Ses Bozukluğu","definition":"Sesin perdesi, tınısı veya kalitesindeki anormallikler; vokal patolojiler dahil."},
  {"term":"Akıcılık Bozukluğu","definition":"Kekemelik (disfluency) gibi konuşmanın akışında kesintilerle kendini gösteren bozukluklar."},
  {"term":"Pragmatik Dil Bozukluğu","definition":"Sosyal iletişim kurallarını ve bağlamı kullanmada güçlük; uygun dil kullanımında sorunlar."},
  {"term":"Fonoloji","definition":"Dilin ses sistemini ve seslerin düzenlenmesini inceleyen alan."},
  {"term":"Fonem","definition":"Bir dilde anlamı ayırt eden en küçük ses birimi."},
  {"term":"İşitsel İşleme Bozukluğu","definition":"İşitme eşiği normal iken işitsel bilgiyi algılama ve işlemlemede güçlük."},
  {"term":"Sözcük Dağarcığı","definition":"Bir kişinin bildiği ve kullandığı kelimelerin toplamı; lexicon."},
  {"term":"Semantik","definition":"Sözcük ve cümlelerin anlamıyla ilgilenen dilbilim alanı."},
  {"term":"Morfoloji","definition":"Kelime yapısını ve kelime içi anlam birimlerini inceleyen dilbilim dalı."},
  {"term":"Sentaks","definition":"Cümle yapısını ve sözcüklerin bir araya gelme kurallarını inceleyen dilbilim alanı."},
  {"term":"Ekolali","definition":"Başka birinin söylediğini tekrarlama davranışı; gelişimsel ve nörolojik durumlarda görülebilir."},
  {"term":"Stereotipi","definition":"Tekrarlayıcı ve amaçsız davranış veya konuşma örüntüleri."},
  {"term":"Dil Gecikmesi","definition":"Beklenen döneme göre dil becerilerinin geride kalması durumu."},
  {"term":"Konuşma Terapisi","definition":"Konuşma, dil ve iletişim bozukluklarını değerlendirme ve tedavi etme süreci."},
  {"term":"Prosodi","definition":"Konuşmadaki ritim, vurgu, tonlama ve melodi özellikleri."},
  {"term":"Artikülasyon Bozukluğu","definition":"Bazı konuşma seslerinin yanlış üretilmesi veya yer değiştirmesi durumu."},
  {"term":"Fonetik","definition":"Seslerin fiziksel üretimi ve özellikleriyle ilgilenen bilim dalı."},
  {"term":"Oral Motor Fonksiyon","definition":"Ağız kaslarının kontrolü ve hareket yeteneği; konuşma için önemlidir."},
  {"term":"Konuşma Hızı","definition":"Konuşmanın ölçülebilir hızı; kelime/dakika gibi metriklerle değerlendirilir."},
  {"term":"Nefes Kontrolü","definition":"Konuşma sırasında doğru solunum ve hava akışını yönetme becerisi."},
  {"term":"Resonans Bozukluğu","definition":"Nazalizasyon veya oral resonansta anormallikler; konuşma tınısını etkiler."},
  {"term":"Değerlendirme","definition":"Konuşma ve dil becerilerinin standart testler ve gözlemlerle ölçülmesi süreci."},
  {"term":"Terapi Hedefi","definition":"Bireysel terapi planında belirlenen ulaşılmak istenen beceri veya davranış değişikliği."},
  {"term":"Bireyselleştirilmiş Eğitim Planı (BEP)","definition":"Öğrencinin özel eğitim ihtiyaçlarına göre hazırlanan hedef ve destek planı."},
  {"term":"Rehabilitasyon","definition":"Fonksiyonun yeniden kazanılmasına yönelik süreklilik arz eden tedavi süreci."},
  {"term":"Augmentatif ve Alternatif İletişim (AAC)","definition":"Konuşma yerine veya yanında kullanılan iletişim sistemleri ve araçları (resimler, cihazlar)."},
  {"term":"Nörolojik Dil Bozuklukları","definition":"Beyin hasarına bağlı ortaya çıkan afazi ve benzeri dil bozuklukları."},
  {"term":"Hedef Odaklı Terapi","definition":"Belirli bir iletişim hedefi üzerine yapılandırılmış kısa veya uzun dönemli terapi yöntemi."},
  {"term":"Konuşma Sesi Bozukluğu","definition":"Çocuklarda veya yetişkinlerde ses üretimindeki bozuklukları kapsayan durum."},
  {"term":"Ses Hijyeni","definition":"Ses sağlığını korumaya yönelik davranış ve uygulamalar."},
  {"term":"Ses Terapisi","definition":"Vokal eğitim ve tedavi teknikleri ile ses problemlerinin düzeltilmesi yöntemi."},
  {"term":"Dil Gelişim Aşamaları","definition":"Çocuklarda dilin doğal olarak geliştiği evreler ve beklenen beceriler."},
  {"term":"İletişim Stratejileri","definition":"Kişilerin iletişim etkinliğini artırmak için kullandıkları yöntemler (jest, yazı, destekleyici araçlar)."},
  {"term":"Bağlamsal Dil Öğretimi","definition":"Dil becerilerini gerçek hayat bağlamında öğretmeye yönelik yöntemler."},
  {"term":"Telerehabilitasyon","definition":"Uzaktan sunulan konuşma ve dil terapisi hizmetleri."},
  {"term":"Dil Değerlendirme Araçları","definition":"Standart testler, anketler ve gözlem formları gibi ölçüm araçları."},
  {"term":"Dil Bozukluğu","definition":"Konuşma, dil veya iletişim becerilerinde işlevselliği etkileyen genel durum."},
  {"term":"Dil Terapisti","definition":"Konuşma ve dil bozuklukları konusunda değerlendirme ve tedavi sağlayan uzman."},

